pipeline{
    agent any
    environment{
       PATH = "/usr/share/maven/bin:$PATH"
    }
    stages{
        stage("Git checkout"){
            steps{
               git credentialsId: 'gith', url: 'https://github.com/sayamsubbu/myweb/'
                
            }
        }
    stage("Maven buld"){
            steps{
                
                sh "cd /root/mvn/subbu"
		sh "mvn clean package"
		
                
                
            }
        }
	
     
    }
}
